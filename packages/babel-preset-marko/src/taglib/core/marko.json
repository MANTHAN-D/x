{
  "taglib-id": "marko-core",
  "<import>": {
    "node-factory": "./parse-import.js",
    "parse-options": {
      "rootOnly": true,
      "relaxRequireCommas": true,
      "ignoreAttributes": true,
      "rawOpenTag": true
    }
  },
  "<export>": {
    "node-factory": "./parse-export.js",
    "parse-options": {
      "rootOnly": true,
      "relaxRequireCommas": true,
      "ignoreAttributes": true,
      "rawOpenTag": true
    }
  },
  "<class>": {
    "node-factory": "./parse-class.js",
    "parse-options": {
      "rootOnly": true,
      "rawOpenTag": true,
      "openTagOnly": true,
      "ignoreAttributes": true
    }
  },
  "<static>": {
    "node-factory": "./parse-static.js",
    "parse-options": {
      "rootOnly": true,
      "rawOpenTag": true,
      "openTagOnly": true,
      "ignoreAttributes": true
    }
  },
  "<style>": {
    "node-factory": "./parse-style.js",
    "parse-options": {
      "rootOnly": true,
      "rawOpenTag": true,
      "ignoreAttributes": true
    }
  },
  "<macro>": {
    "transformer": "./macro/transform.js",
    "code-generator": "./macro/translate.js",
    "parse-options": {
      "rootOnly": true
    }
  },
  "<if>": {
    "code-generator": "./conditional/translate-if.js",
    "@if": "expression"
  },
  "<else>": {
    "code-generator": "./conditional/translate-else.js",
    "@if": "expression"
  },
  "<for>": {
    "code-generator": "./translate-for.js",
    "@of": "array",
    "@in": "object",
    "@from": "number",
    "@to": "number",
    "@step": "number",
    "@by": "string"
  },
  "<html-comment>": {
    "code-generator": "./translate-html-comment.js"
  },
  "<marko-preserve-whitespace>": {
    "code-generator": "./translate-marko-preserve-whitespace.js",
    "parse-options": {
      "preserveWhitespace": true
    }
  },
  "<key>": {
    "transformer": "./transform-key.js",
    "@key": "string"
  },
  "<*>": {
    "transformer": "./attributes/transform.js"
  }
}
