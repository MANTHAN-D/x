{
  "taglib-id": "marko-core",
  "<import>": {
    "node-factory": "./parse-import.js",
    "parse-options": {
      "rootOnly": true,
      "relaxRequireCommas": true,
      "ignoreAttributes": true,
      "rawOpenTag": true
    }
  },
  "<export>": {
    "node-factory": "./parse-export.js",
    "parse-options": {
      "rootOnly": true,
      "relaxRequireCommas": true,
      "ignoreAttributes": true,
      "rawOpenTag": true
    }
  },
  "<class>": {
    "node-factory": "./parse-class.js",
    "parse-options": {
      "rootOnly": true,
      "ignoreAttributes": true,
      "rawOpenTag": true
    }
  },
  "<static>": {
    "node-factory": "./parse-static.js",
    "parse-options": {
      "rootOnly": true,
      "ignoreAttributes": true,
      "rawOpenTag": true
    }
  },
  "<if>": {
    "transformer": "./conditional/transform-if.js",
    "@if": "expression"
  },
  "<else>": {
    "transformer": "./conditional/transform-else.js",
    "@if": "expression"
  },
  "<for>": {
    "transformer": "./transform-for.js",
    "@of": "array",
    "@in": "object",
    "@from": "number",
    "@to": "number",
    "@step": "number",
    "@by": "string"
  },
  "<html-comment>": {
    "transformer": "./transform-html-comment.js"
  },
  "<marko-preserve-whitespace>": {
    "transformer": "./transform-marko-preserve-whitespace.js",
    "parse-options": {
      "preserveWhitespace": true
    }
  },
  "<*>": {
    "transformer": "./directives/transform-directives.js"
  }
}
