{
  "taglib-id": "marko-core",
  "<import>": {
    "node-factory": "./parse-import.js",
    "parse-options": {
      "rootOnly": true,
      "rawOpenTag": true,
      "openTagOnly": true,
      "ignoreAttributes": true,
      "relaxRequireCommas": true
    }
  },
  "<export>": {
    "node-factory": "./parse-export.js",
    "parse-options": {
      "rootOnly": true,
      "rawOpenTag": true,
      "openTagOnly": true,
      "ignoreAttributes": true,
      "relaxRequireCommas": true
    }
  },
  "<class>": {
    "node-factory": "./parse-class.js",
    "parse-options": {
      "rootOnly": true,
      "rawOpenTag": true,
      "openTagOnly": true,
      "ignoreAttributes": true
    }
  },
  "<static>": {
    "node-factory": "./parse-static.js",
    "parse-options": {
      "rootOnly": true,
      "rawOpenTag": true,
      "openTagOnly": true,
      "ignoreAttributes": true
    }
  },
  "<style>": {
    "transformer": "./transform-style.js",
    "parse-options": {
      "rootOnly": true,
      "rawOpenTag": true,
      "ignoreAttributes": true
    }
  },
  "<macro>": {
    "transformer": "./macro/transform.js",
    "code-generator": "./macro/translate.js",
    "parse-options": {
      "rootOnly": true
    },
    "@name": "string"
  },
  "<if>": {
    "code-generator": "./conditional/translate-if.js",
    "@if": "expression"
  },
  "<else>": {
    "code-generator": "./conditional/translate-else.js",
    "@if": "expression"
  },
  "<for>": {
    "code-generator": "./translate-for.js",
    "@of": "array",
    "@in": "object",
    "@from": "number",
    "@to": "number",
    "@step": "number",
    "@by": "string"
  },
  "<html-comment>": {
    "code-generator": "./translate-html-comment/index.js",
    "attribute-groups": ["html-attributes"]
  },
  "<key>": {
    "transformer": "./transform-key.js",
    "@key": "string"
  },
  "<no-update>": {
    "@key": "string",
    "@if": "boolean",
    "@body-only": "boolean",
    "autocomplete": []
  },
  "<component-globals>": {
    "autocomplete": []
  },
  "<init-components>": {
    "@immediate": "boolean"
  },
  "<body>": {
    "transformer": "./transform-body.js"
  },
  "<*>": {
    "transformer": "./attributes/transform.js"
  }
}
